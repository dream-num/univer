{
    "name": "@univerjs/base-ui",
    "version": "0.0.1",
    "description": "UniverSheet normal plugin UI manager",
    "keywords": [
        "univer",
        "ui"
    ],
    "author": "DreamNum <msoffice2@126.com>",
    "license": "MIT",
    "main": "./src/index.ts",
    "module": "./src/index.ts",
    "publishConfig": {
        "main": "./lib/univer-base-ui.cjs.js",
        "module": "./lib/univer-base-ui.js",
        "types": "./lib/index.d.ts",
        "exports": {
            ".": {
                "require": "./lib/univer-base-ui.cjs.js",
                "default": "./lib/univer-base-ui.js"
            },
            "./*": "./lib/*"
        },
        "access": "public"
    },
    "exports": {
        ".": "./src/index.ts",
        "./*": "./src/*"
    },
    "directories": {
        "lib": "lib"
    },
    "files": [
        "lib"
    ],
    "private": false,
    "scripts": {
        "jest": "jest -c jest.config.js",
        "esbuild": "node ./esbuild.config.js",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build"
    },
    "devDependencies": {
        "@storybook/addon-essentials": "^7.4.0",
        "@storybook/addon-interactions": "^7.4.0",
        "@storybook/addon-links": "^7.4.0",
        "@storybook/addon-onboarding": "^1.0.8",
        "@storybook/blocks": "^7.4.0",
        "@storybook/react": "^7.4.0",
        "@storybook/react-vite": "^7.4.0",
        "@storybook/testing-library": "^0.2.0",
        "@testing-library/react": "^14.0.0",
        "@types/jest": "^29.5.4",
        "@types/node": "^18.14.0",
        "@types/react": "^18.2.21",
        "@types/react-dom": "^18.2.7",
        "jest": "29.6.4",
        "jest-css-modules-transform": "^4.4.2",
        "jest-environment-jsdom": "^29.6.4",
        "less": "^4.1.1",
        "storybook": "^7.4.0",
        "ts-jest": "29.1.1",
        "ts-node": "^10.9.1"
    },
    "dependencies": {
        "@univerjs/base-render": "workspace:^0.0.1",
        "@univerjs/core": "workspace:^0.0.1",
        "@wendellhu/redi": "^0.12.7",
        "react": "^18.2.0",
        "react-colorful": "^5.3.0",
        "react-dom": "^18.2.0",
        "rxjs": "^7.8.1"
    }
}
