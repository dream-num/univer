            },
            "./*": {
                "import": "./lib/es/*",
                "require": "./lib/cjs/*",
                "types": "./lib/types/index.d.ts"
            }
        }
    },
    "directories": {
        "lib": "lib"
    },
    "files": [
        "lib"
    ],
