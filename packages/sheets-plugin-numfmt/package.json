{
    "name": "@univerjs/sheets-plugin-numfmt",
    "version": "0.0.1",
    "description": "UniverSheet normal plugin UI manager",
    "keywords": [],
    "author": "DreamNum <developer@univer.ai>",
    "license": "MIT",
    "main": "./lib/cjs/index.js",
    "module": "./lib/esm/index.js",
    "types": "./lib/index.d.ts",
    "publishConfig": {
        "access": "public",
        "main": "./lib/cjs/index.js",
        "module": "./lib/esm/index.js",
        "exports": {
            ".": {
                "import": "./lib/esm/index.js",
                "require": "./lib/cjs/index.js",
                "types": "./lib/types/index.d.ts"
            },
            "./*": {
                "import": "./lib/esm/*",
                "require": "./lib/cjs/*",
                "types": "./lib/types/index.d.ts"
            },
            "./locale/*": {
                "import": "./lib/esm/locale/*.js",
                "require": "./lib/cjs/locale/*.js",
                "types": "./lib/types/index.d.ts"
            }
        }
    },
    "exports": {
      ".": {
        "require": "./lib/univer-sheets-plugin-numfmt.cjs.js",
        "default": "./lib/univer-sheets-plugin-numfmt.js"
      },
      "./*": "./lib/*",
      "./locale/*": "./lib/locale/*.js"
    },
    "directories": {
        "lib": "lib"
    },
    "files": [
        "lib"
    ],
    "private": false,
    "scripts": {
        "test": "vitest run",
        "esbuild": "node ./esbuild.config.mjs && tsc -p tsconfig.esbuild.json"
    },
    "dependencies": {
        "@univerjs/base-numfmt-engine": "workspace:*",
        "@univerjs/base-render": "workspace:*",
        "@univerjs/base-sheets": "workspace:*",
        "@univerjs/base-ui": "workspace:*",
        "@univerjs/core": "workspace:*",
        "@univerjs/design": "workspace:*",
        "@univerjs/ui-plugin-sheets": "workspace:*",
        "@wendellhu/redi": "^0.12.11"
    },
    "devDependencies": {
        "@types/react": "^18.2.38",
        "@vitest/coverage-istanbul": "^0.34.6",
        "esbuild": "^0.19.7",
        "less": "^4.2.0",
        "react": "^18.2.0",
        "ts-node": "^10.9.1",
        "typescript": "^5.3.2",
        "vitest": "^0.34.6"
    },
    "peerDependencies": {
        "react": ">=16.9.0"
    }
}
